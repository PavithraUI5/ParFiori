					</IconTabFilter>
				</items>
			</IconTabBar>
			<!--<List items="{/listData}" itemPress="onItemPress">
					<ObjectListItem type="Navigation" title="{name}" icon="{myIcon}"></ObjectListItem>
			</List>-->
