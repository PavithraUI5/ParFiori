	<Page title="Nandana's Beauty Parlor">
		<content>
			<IconTabBar>
				<items>
					<IconTabFilter text="Home" icon="">
						<Image src="http://www.naturals.in/wp-content/uploads/2015/05/naturals-mobi.jpg" width="100%" height="100%"></Image>
					</IconTabFilter>
					<IconTabFilter text="New Registration" icon="">
						<core:Fragment fragmentName="cn.par.fragments.Registration" type="XML"></core:Fragment>
					</IconTabFilter>
