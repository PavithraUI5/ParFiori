					</IconTabFilter>
					<IconTabFilter text="Buy Products" icon="">
						<core:Fragment fragmentName="cn.par.fragments.BuyProduct" type="XML"></core:Fragment>
					</IconTabFilter>
					<IconTabFilter text="Feedback" icon="">
